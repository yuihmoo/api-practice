<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>Apache-Axis</display-name>
    <servlet>
        <display-name>Apache-Axis Servlet</display-name>
        <servlet-name>StartController</servlet-name>
        <servlet-class>kr.co.nandsoft.api.controller.StartController</servlet-class>
    </servlet>
    <servlet>
        <display-name>Apache-Axis Servlet</display-name>
        <servlet-name>AllViewController</servlet-name>
        <servlet-class>kr.co.nandsoft.api.controller.AllViewController</servlet-class>
    </servlet>
    <servlet>
        <display-name>Apache-Axis Servlet</display-name>
        <servlet-name>ServiceViewController</servlet-name>
        <servlet-class>kr.co.nandsoft.api.controller.ServiceViewController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StartController</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AllViewController</servlet-name>
        <url-pattern>/all</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServiceViewController</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>
</web-app>